{
  "name": "__MSG_appName__",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://tictac.wikimedia.de/web*"
      ],
      "js": [
        "scripts/correctdate.js",
        "scripts/dayofweek.js",
        "scripts/quicktableedit.js"
      ],
      "css": [
        "styles/style.css"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
